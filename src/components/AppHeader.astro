---
import BrandLogo from "./BrandLogo.astro";
import Container from "./Container.astro";
import { button, ghostButton, outlinedButton } from "@tailus/themer-button"
import {cn} from "@lib/utils"
import Frame from "./Frame.astro";

const links = [
    {
        to: "/#home",
        label: "Product",
    },
    {
        to: "/#features",
        label: "Features",
    },
    {
        to: "/#solution",
        label: "Solution",
    },
    {
        to: "/#reviews",
        label: "Reviews",
    },
];
---
<header>
    <nav class="absolute z-50 w-full lg:bg-transparent dark:lg:bg-transparent">
        <Container>
            <div class="relative flex flex-wrap items-center justify-center gap-6 py-3 md:gap-0 md:py-4 lg:py-8">
                <div class="relative z-20 flex w-full items-center justify-center md:px-0 lg:w-max">
                    <BrandLogo />
                </div>
            </div>
        </Container>
    </nav>
</header>